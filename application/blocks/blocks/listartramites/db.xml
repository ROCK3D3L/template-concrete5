        </field>
        <field name="bID" type="I"><unsigned /></field>
        <field name="nombre" type="C" size="100"></field>        
        <field name="sitio_web" type="C" size="100"></field>  
        <field name="descripcion" type="X" size="300"></field>
        <field name="institucion" type="C" size="200"></field>          
        <field name="tipo_servicio" type="C" size="200"></field>  
