        <field name="documentacion" type="C" size="100"></field>  
        <field name="tiempo_estimado" type="C" size="100"></field>  
        <field name="precio" type="C" size="100"></field>  
        <field name="forma_pago" type="C" size="100"></field>  
        <field name="atencion" type="C" size="100"></field>  
        <field name="correo_servicio" type="C" size="100"></field>  
        <field name="formulario" type="X" size="150"></field>  
        <field name="online" type="C" size="100"></field>  
        <field name="orden" type="I">
            <notnull />
        </field>   
		<field name="fID" type="I"><UNSIGNED /><default value="0" /></field>
    </table>
</schema>