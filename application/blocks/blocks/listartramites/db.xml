<?xml version="1.0"?>
<schema version="0.3">
    <table name="btListarTramites">
        <field name="bID" type="I">
            <key />
            <unsigned />
        </field>
        <field name="titulo" type="C" size="50"></field>
		<field name="cantItemsPag" type="I"></field>
    </table>
    
