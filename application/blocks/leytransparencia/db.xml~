<?xml version="1.0"?>
<schema version="0.3">
	<table name="btMenu">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="titulo" type="C" size="50"></field>
        <field name="color" type="C" size="7"></field>
	</table>
	
	<table name="btMenuEntries">
		<field name="id" type="I">
			<key />
			<unsigned />
			<autoincrement />
		</field>
		<field name="bID" type="I"><unsigned /></field>
		<field name="nombre" type="C" size="50"></field>		
		<field name="orden" type="I">
			<notnull />
			</field>
		<field name="idPadre" type="I"></field>	
		<field name="subMenu" type="L"></field>	
		<field name="link" type="C" size="200">
			<notnull />
		</field>
	</table>
</schema>