<?xml version="1.0"?>
<schema version="0.3">
	<table name="btCalendario">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
	</table>
	
	<table name="btCalendarioEntries">
		<field name="id" type="I">
			<key />
			<unsigned />
			<autoincrement />
		</field>
		<field name="bID" type="I"><unsigned /></field>
		<field name="titulo" type="C" size="500">
			<notnull />
			</field>	
		<field name="descripcion" type="C" size="500">
			</field>		
		<field name="horaInicio" type="C" size="6">
			<notnull />
			</field>
		<field name="horaFin" type="C" size="6">
			<notnull />
			</field>
		<field name="fecha" type="C" size="10"></field>	
		<field name="lugar" type="C" size="200">
			<notnull />
		</field>
	</table>
</schema>