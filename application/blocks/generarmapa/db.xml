<?xml version="1.0"?>
<schema version="0.3">
	<table name="btMapa">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="titulo" type="C" size="100"></field>
		<field name="zoom" type="I"></field>
	</table>
	
	<table name="btMapaEntries">
		<field name="id" type="I">
			<key />
			<unsigned />
			<autoincrement />
		</field>
		<field name="bID" type="I"><unsigned /></field>
		<field name="nombre" type="C" size="50"></field>		
        <field name="cuerpo" type="X2"></field>	
		<field name="x" type="C" size="15">
			<notnull />
			</field>
		<field name="y" type="C" size="15">
			<notnull />
			</field>
		<field name="orden" type="I"></field>
        <field name="icono" type="I"></field>	
	</table>
</schema>
